
  <%- if controller_name != 'sessions'  %>
      <%= link_to  t('users.sessions.new.boton_login'), new_session_path(resource_name), class: 'btn btn-info' %>
  <% end -%>
  
  <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
      <%= link_to  t('users.sessions.new.registrarse'), new_registration_path(resource_name), class: 'btn btn-info' %>
  <% end -%>
  
  <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
      <%= link_to t('users.sessions.new.olvide_contrasena'), new_password_path(resource_name), class: 'btn btn-info' %>
    </div>
  <% end -%>
  
  <%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
      <%= link_to "No recibiste las instrucciones de confirmación?", new_confirmation_path(resource_name), class: 'btn btn-warning' %>
  <% end -%>
  
  <%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
      <%= link_to "No recibí las instrucciones de desbloqueo", new_unlock_path(resource_name), class: 'btn btn-warning' %>
  <% end -%>
  
  <%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
        <%= link_to "Entrar con #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider), class: 'btn btn-primary' %>
    <% end -%>
  <% end -%>